Executable = DailyPythonScripts/experimental/mergeBATOutputFilesOnDICE/mergeJob.sh
Universe = vanilla
Output = mergeBAToutput.job.$(cluster).$(process).out
Error = mergeBAToutput.job.$(cluster).$(process).err
Log = mergeBAToutput.job.$(cluster).$(process).log
arguments = $(process) 8

transfer_input_files = dps.tar
should_transfer_files = YES
when_to_transfer_output = ON_EXIT_OR_EVICT

