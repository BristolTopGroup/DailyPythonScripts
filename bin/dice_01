#!/bin/bash
echo "Tarring DailyPythonScripts..."
tar -cf dps.tar ../DailyPythonScripts/ --exclude ../DailyPythonScripts/data --exclude ../DailyPythonScripts/plots --exclude ../DailyPythonScripts/tables
echo "Submitting jobs to DICE..."
condor_submit experimental/condor/01_fit.description
echo "Done."
condor_q

